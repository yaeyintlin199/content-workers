{
    "name": "@content-workers/auth-github",
    "version": "1.0.1",
    "description": "The official GitHub authentication provider plugin for Lucid CMS",
    "type": "module",
    "types": "./dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc && tsdown",
        "dev": "tsdown --watch",
        "format": "biome format ./src",
        "format:fix": "biome format --write ./src",
        "lint": "biome lint ./src",
        "lint:fix": "biome lint --write ./src"
    },
    "files": [
        "dist",
        "assets"
    ],
    "author": "Lucid",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/buildlucid/lucid-cms.git",
        "directory": "packages/auth-github"
    },
    "keywords": [
        "CMS",
        "Lucid CMS",
        "Lucid",
        "GitHub",
        "Authentication",
        "Provider",
        "Plugin"
    ],
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@content-workers/config": "*",
        "@content-workers/core": "*",
        "@types/node": "^22.15.2",
        "tsdown": "^0.15.7",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "@content-workers/core": "^0.12.1-alpha.1"
    },
    "volta": {
        "node": "24.11.1"
    },
    "engines": {
        "node": "^24.0.0"
    },
    "license": "MIT"
}